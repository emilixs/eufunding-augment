# Environment Variables Example
# Copy this file to .env and fill in your actual values

# Lista Firme API Configuration
# Get your API key from: https://membri.listafirme.ro/
LISTA_FIRME_API_KEY=your_lista_firme_api_key_here
LISTA_FIRME_BASE_URL=https://www.listafirme.ro/api

# LLM API Keys (for RubyLLM - optional)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Rails Configuration
RAILS_ENV=development
RAILS_MAX_THREADS=5

# Database (SQLite is used by default in development)
# DATABASE_URL=sqlite3:storage/development.sqlite3

# Application Settings
